<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amrita.amail">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_app_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".editcon"></activity>
        <activity android:name=".allmails" />
        <activity android:name=".Unead" />
        <activity
            android:name=".Main2Activity"
            android:parentActivityName=".afterLogin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity
            android:name=".afterLogin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".dialog"
            android:theme="@style/Theme.AppCompat.Dialog.Alert" />
        <activity android:name=".Forgotpasswd" />
        <activity android:name=".PhonenumberVerification" />
        <activity android:name=".kyctest" />
    </application>

</manifest>